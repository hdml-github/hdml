    "test": "lerna run test",
    "test_dbg": "lerna run test_dbg",
    "clear": "lerna run clear",
    "build": "lerna run build",
    "publish": "lerna publish from-package --yes --no-verify-access",
    
    "up:develop": "clear && docker compose -f ./dev.project/compose:develop.yaml up -d",
    "down:develop": "clear && docker compose -f ./dev.project/compose:develop.yaml down",

    "build:devtools": "clear && export DOCKER_BUILDKIT=1 && docker build . -f ./docker/devtools/Dockerfile -t imazzine/hdml.io-devtools",
    "debug:devtools": "clear && docker run --network host  -v .:/var/lib/hdml -it imazzine/hdml.io-devtools /bin/sh",
    
    "up:testing": "rm -rf ./dev.project/.log/*.log && clear && docker compose -f ./dev.project/compose:testing.yaml up -d",
    "down:testing": "clear && docker compose -f ./dev.project/compose:testing.yaml down",
    "build:testing": "clear && export DOCKER_BUILDKIT=1 && docker build . -f ./docker/testing/Dockerfile -t imazzine/hdml.io:testing",
    "debug:testing": "clear && docker run --network host -p 8887:8887 -p 8888:8888 -v /home/buntarb/Projects/hdml-github/hdml/dev.project:/var/lib/hdml -v /home/buntarb/Projects/hdml-github/hdml/dev.project/.catalog:/etc/trino/catalog -it imazzine/hdml.io:testing /bin/sh",
    "exec:testing": "clear && docker run --network host -p 8887:8887 -p 8888:8888 -v /home/buntarb/Projects/hdml-github/hdml/dev.project:/var/lib/hdml -v /home/buntarb/Projects/hdml-github/hdml/dev.project/.catalog:/etc/trino/catalog imazzine/hdml.io:testing"
  }
