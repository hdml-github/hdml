  "scripts": {
    "bootstrap": "lerna bootstrap",
    "dev": "lerna run dev",
    "lint": "lerna run lint",
    "docs": "lerna run docs",
    "compile_asc": "lerna run compile_asc",
    "compile_cjs": "lerna run compile_cjs",
