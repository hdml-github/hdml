{
  "name": "@hdml/arrow",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "apache-arrow": "^7.0.0"
  },
  "scripts": {
    "lint": "node ../../node_modules/.bin/eslint -c ../../.eslintrc.js --fix --parser-options=tsconfigRootDir:'./tsconfig' 'src/**/*.ts'",
    "docs": "../../node_modules/.bin/typedoc ./src/index.ts --tsconfig ./tsconfig/cjs.json --out ./docs --includes ./",
    "compile_asc": "asc \"./src/assembly.as\" --exportRuntime --extension as --target release",
    "compile_cjs": "tsc -b ./tsconfig/cjs.json",
    "compile_esm": "tsc -b ./tsconfig/esm.json",
    "compile_dts": "tsc -b ./tsconfig/dts.json",
    "compile_tst": "tsc -b ./tsconfig/tst.json",
    "compile_all": "npm run compile_cjs && npm run compile_esm && npm run compile_dts",
    "bundlify": "node ../../scripts/bundlify.js",
    "closurify": "node ../../scripts/closurify.js",
    "test:unit": "../../node_modules/.bin/jest --config ../../.jestrc.js --env ./.jestenv.js --roots ./packages/arrow/tst --coverageDirectory ./packages/arrow/coverage",
    "test:i9n": "cd ../.. && npm run test:i9n:run -- --spec ./integration/tests/arrow/*.spec.js",
    "build": "npm run lint && npm run compile_tst && npm run test:unit -- --coverage && npm run compile_all && npm run bundlify && npm run closurify && npm run test:i9n && npm run docs"
  },
  "types": "dts/index.d.ts",
  "module": "esm/index.js",
  "main": "cjs/index.js"
}