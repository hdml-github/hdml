{
  "name": "@hdml/table",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "lit": "^2.2.2",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "dev": "../../node_modules/.bin/web-dev-server --config ../../.dev.config.js --open ./index.html",
    "lint": "node ../../node_modules/.bin/eslint -c ../../.eslintrc.js --fix --parser-options=tsconfigRootDir:'./tsconfig' 'src/**/*.ts'",
    "docs": "../../node_modules/.bin/typedoc ./src/index.ts --tsconfig ./tsconfig/cjs.json --out ./docs --includes ./",
    "compile_asc": "asc \"./src/assembly.as\" --exportRuntime --extension as --target release",
    "compile_cjs": "tsc -b ./tsconfig/cjs.json",
    "compile_esm": "tsc -b ./tsconfig/esm.json",
    "compile_dts": "tsc -b ./tsconfig/dts.json",
    "compile_all": "npm run compile_cjs && npm run compile_esm && npm run compile_dts",
    "compile_web": "npm run compile_esm && npm run bundlify && npm run closurify",
    "bundlify": "node ../../scripts/bundlify.js",
    "closurify": "node ../../scripts/closurify.js",
    "test": "../../node_modules/.bin/wtr --config ../../.test.config.js \"src/**/*.test.html\"",
    "test:watch": "npm run test -- --watch",
    "build": "npm run lint && npm run test -- --coverage && npm run compile_all && npm run bundlify && npm run closurify && npm run docs"
  },
  "types": "dts/index.d.ts",
  "module": "esm/index.js",
  "main": "cjs/index.js"
}