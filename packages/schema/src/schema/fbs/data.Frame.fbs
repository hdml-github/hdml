include "./data.Field.fbs";
include "./data.FilterClause.fbs";

namespace data;

/// Data frame type.
table Frame {
  name: string;
  host: string;
  source: string;
  offset: ulong;
  limit: ulong;
  fields: [data.Field];
  filter_by: data.FilterClause;
  group_by: [data.Field];
  split_by: [data.Field];
  sort_by: [data.Field];
  parent: Frame;
}